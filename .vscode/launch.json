  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug webui_demo",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/examples/webui_demo",
      "env": {}
    },
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/pkg/cmd",
      "args": [
        "-webui",
        "-download-limit-bps",
        "300000"
      ]
    }
  ]
}